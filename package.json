{
  "name": "rainbow-roles",
  "version": "1.3.1",
  "description": "Make Discord server roles change color!",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "DEBUG=colors,hello,guild,bot,bot-command,die node index.js",
    "start-verbose": "DEBUG=update-color-error,colors,hello,guild,bot,bot-command,stats,invites,role-built,die node index.js",
    "start-build": "DEBUG=update-color-error,colors,hello,guild,bot,bot-command,stats,invites,role-built,scheme-build,die node index.js",
    "start-dev": "DEBUG=*,-follow-redirects node index.js",
    "format": "npx standardx --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luawtf/rainbow-roles.git"
  },
  "keywords": [
    "discord",
    "bot",
    "color",
    "rainbow"
  ],
  "author": "Jack MacDougall <luawhat@gmail.com> (https://lua.wtf/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luawtf/rainbow-roles/issues"
  },
  "homepage": "https://github.com/luawtf/rainbow-roles#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "color-convert": "^2.0.0",
    "debug": "^4.1.1",
    "discord.js": "^11.5.1",
    "json5": "^2.1.0",
    "standardx": "^3.0.1",
    "tinygradient": "^1.0.0",
    "titlecase": "^1.1.3"
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        "error",
        4
      ]
    }
  }
}
